#include <iostream>
#include <string>


class Point{
private:
    int x, y;
public:
    int getX(int x){
        return this->x;
    }

    int getY(int y){
        return this->y;
    }
    std :: string to_string(int x, int y){
        std::cout << "(" << x << ", "<< y << ")";
    }
    ~Point(){

    }
};


int main() {

    int x = 15;
    int y = 20;

    Point firstPoint;

    firstPoint.getX(x);
    firstPoint.getY(y);
    firstPoint.to_string(x,y);
    return 0;
}

